.ws-size {
    width: 977px;
    height: 510px;
}

.welcome-screen-begin-button_margin {
    margin-top: 300px;
}

.welcome-screen-image-size {
    width: 200px;
    height: 300px;
    background-image: var(--ws-npc-orientation-image);
}

.ws-content-size {
    height: 278px;
    margin-top: 20px;
}

.ws-message-text {
    color: var(--ws-text-color);
    margin-left: 30px;
    padding-right: 50px;
}

.ws-header-text {
    margin-left: 30px;
    color: var(--ws-text-color);
}

.ws-background-dim {
