}

.ws-footer {
    align-content: center;
}

.ws-content-image {
    width: 300px;
    height: 220px;
    align-content: stretch;
    margin-top: 14px;
    background-image: var( --ws-star-image);
}

.ws-button {
    width: 213px;
    height: 48px;
}

.ws-content-text-container {
    margin-top: 30px;
}